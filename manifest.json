   "icons": {
      "128": "img/logo_128x128.png",
      "16": "img/logo_16x16.png",
      "48": "img/logo_48x48.png"
   },
   "incognito": "split",
   "key": "",
   "manifest_version": 2,
   "name": "VRC REST Client",
   "permissions": [ "\u003Call_urls>", "clipboardWrite", "storage", "unlimitedStorage" ],
   "short_name": "YARC",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.1.5"
}
